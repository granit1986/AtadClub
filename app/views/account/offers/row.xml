<Alloy>
	<TableViewRow toUserId="{toUserId}" rowId="{id}" hasChild="true" onClick="onClick" id="row" isReaded="{isReaded}" state="{state}">
		<Label id="advert" text="{advert}"></Label>
		<Label id="price" text="{price}"></Label>
		<Label id="date" text="{date}"></Label>
		<ImageView id="img" image="{image}"></ImageView>
	</TableViewRow>
</Alloy>