var upload={types:{advert:1,deal:2,logo:3,product:4,barter:6},start:function(t){var l=Ti.Network.createHTTPClient({timeout:36e5});require("alloy/moment"),l.ondatastream=function(){t.ondatastream&&t.ondatastream()},l.onerror=function(l){t.onerror&&t.onerror(l)},l.onload=function(){t.onload&&t.onload()},l.onreadystatechange=function(){t.onreadystatechange&&t.onreadystatechange()},l.onsendstream=function(l){t.onsendstream&&t.onsendstream(l)},l.open("POST","http://"+Titanium.App.serverDomain+"/api/"+Titanium.App.ApiVersion+"/"+Alloy.Globals.core.apiToken()+"/images");for(var e={id:t.id,type:t.type,"delete":t.delete},o=0;o<t.blobs.length;o++)e["file_"+o]=t.blobs[o];l.send(e)}};