var categories={select:function(t){var l="_"+t.id;l in Alloy.Globals.core.selectedCategories||(Alloy.Globals.core.selectedCategories[l]={})},clear:function(t){var l="_"+t.id;l in Alloy.Globals.core.selectedCategories&&delete Alloy.Globals.core.selectedCategories[l]},selected:function(t){var l="_"+t.id;return l in Alloy.Globals.core.selectedCategories?Alloy.Globals.core.selectedCategories[l].length==t.subCategories.length:!1}};