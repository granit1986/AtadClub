var Alloy=require("alloy"),_=require("alloy/underscore")._,model,collection;exports.definition={config:{URL:"http://"+Titanium.App.serverDomain+"/api/"+Titanium.App.ApiVersion+"/{apiToken}/readCounts",columns:{offersCount:"INT",messagesCount:"INT"},adapter:{type:"restapi",collection_name:"counts"}},extendModel:function(e){return _.extend(e.prototype,{}),e},extendCollection:function(e){return _.extend(e.prototype,{}),e}},model=Alloy.M("counts",exports.definition,[]),collection=Alloy.C("counts",exports.definition,model),exports.Model=model,exports.Collection=collection;