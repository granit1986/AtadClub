var Alloy=require("alloy"),_=require("alloy/underscore")._,model,collection;exports.definition={config:{URL:"http://"+Titanium.App.serverDomain+"/api/"+Titanium.App.ApiVersion+"/contactus",columns:{subject:"text",message:"text"},adapter:{type:"restapi"}},extendModel:function(e){return _.extend(e.prototype,{localValidate:function(e){var t=Alloy.Globals.errors,l=Alloy.Globals.core;return l.rxs.empty.test(this.attributes.name)?(e(t.NO_NAME),!1):l.rxs.empty.test(this.attributes.email)?(e(t.NO_EMAIL),!1):l.rxs.email.test(this.attributes.email)?l.rxs.empty.test(this.attributes.phone)?(e(t.NO_PHONE),!1):l.rxs.empty.test(this.attributes.message)?(e(t.NO_MESSAGE),!1):!0:(e(t.INVALID_EMAIL),!1)}}),e}},model=Alloy.M("contactUs",exports.definition,[]),collection=Alloy.C("contactUs",exports.definition,model),exports.Model=model,exports.Collection=collection;