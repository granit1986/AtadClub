var Alloy=require("alloy"),_=require("alloy/underscore")._,model,collection,moment=require("alloy/moment");exports.definition={config:{URL:"http://"+Titanium.App.serverDomain+"/api/"+Titanium.App.ApiVersion+"/adverts",columns:{id:"INTEGER"},adapter:{type:"restapi",collection_name:"publicAdvert",idAttribute:"id"},debug:0},extendModel:function(e){return _.extend(e.prototype,{}),e},extendCollection:function(e){return _.extend(e.prototype,{}),e}},model=Alloy.M("publicAdvert",exports.definition,[]),collection=Alloy.C("publicAdvert",exports.definition,model),exports.Model=model,exports.Collection=collection;